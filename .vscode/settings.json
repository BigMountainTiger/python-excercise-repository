{
  "files.exclude": {
    "**/.git": true,
    "**/.virtual-env/*": true,
    "**/__pycache__": true,
    "**/*.egg-info": true,
    "**/nn_data": true,
    "**/nn_model": true,
    "**/.env-3.*": true,
    "**/c-02-python-pdf-fargate/**/bin": true,
    "**/c-02-python-pdf-fargate/**/obj": true,
    "**/.pytest_cache": true,
    "**/.coverage": true
  },
  "editor.minimap.enabled": false,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  // It looks like the "python.autoComplete.extraPaths" is not taking any effect
  "python.autoComplete.extraPaths": [
    "${workspaceFolder}/vs_code_python_env-def-file/libs"
  ],
  "python.analysis.extraPaths": [
    "${workspaceFolder}/vs_code_python_env-def-file/libs"
  ],
  "python.terminal.activateEnvironment": false
}